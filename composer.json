{
    "name": "steffenbrem/json-api-bundle",
    "description": "Integration of JSON API with Symfony2 using JMS Serializer (FOSRestBundle).",
    "keywords": [
        "rest",
        "jsonapi",
        "json-api"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Steffen Brem",
            "email": "steffenbrem@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-dom": "*",
        "ext-json": "*",
        "symfony/config": "^3.0 || ^4.0",
        "symfony/dependency-injection": "^3.0 || ^4.0",
        "symfony/http-kernel": "^3.0 || ^4.0",
        "symfony/yaml": "^3.0 || ^4.0",
        "symfony/http-foundation": "^3.0 || ^4.0",
        "symfony/property-access": "^3.0 || ^4.0",
        "symfony/validator": "^3.0 || ^4.0",
        "symfony/expression-language": "^3.0 || ^4.0",
        "jms/serializer-bundle": "^3.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "pagerfanta/pagerfanta": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mango\\Bundle\\JsonApiBundle\\": "src/"
        }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
